<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns="http://tizen.org/ns/packages" package="org.tizen.voice-control-panel" version="0.1" install-location="internal-only" api-version="5.0">
        <label>Voice control panel</label>
        <author email="ky85.kim@samsung.com" href="www.samsung.com">Kwangyoun Kim</author>
        <description>Voice control panel Application</description>
        <ui-application appid="org.tizen.voice-control-panel" exec="voice-control-panel" nodisplay="true" multiple="false" type="capp" taskmanage="false">
            <label>Voice control panel</label>
            <metadata key="http://tizen.org/metadata/multi-assistant/name" value="Sample Assistant"/>
            <metadata key="http://tizen.org/metadata/multi-assistant/language" value="ko_KR"/>
            <metadata key="http://tizen.org/metadata/multi-assistant/wakeup_word" value="하이코지"/>
            <metadata key="http://tizen.org/metadata/multi-assistant/wakeup_word" value="하이코지마"/>
            <metadata key="http://tizen.org/metadata/multi-assistant/wakeup_word" value="안녕코지"/>
            <metadata key="http://tizen.org/metadata/multi-assistant/wakeup_word" value="안녕코지마"/>
            <metadata key="http://tizen.org/metadata/multi-assistant/wakeup_engine_appid" value="wakeup-engine-default"/>
            <metadata key="http://tizen.org/metadata/multi-assistant/custom_ui" value="true"/>
            <background-category value="assistant"/>
        </ui-application>
        <privileges>
            <privilege>http://tizen.org/privilege/recorder</privilege>
            <privilege>http://tizen.org/privilege/appmanager.launch</privilege>
            <privilege>http://tizen.org/privilege/inputgenerator</privilege>
            <privilege>http://tizen.org/privilege/window.priority.set</privilege>
            <privilege>http://tizen.org/privilege/voicecontrol.manager</privilege>
        </privileges>
